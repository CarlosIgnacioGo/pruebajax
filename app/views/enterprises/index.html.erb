<p id="notice"><%= notice %></p>

<h1>Listing Enterprises</h1>
<%=render 'form' %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="empresas">
    <% @enterprises.each do |enterprise| %>
      <%= render enterprise %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enterprise', new_enterprise_path %>
